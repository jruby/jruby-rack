Ruby: <%= RUBY_DESCRIPTION %>
Server: <%= request.env['SERVER_SOFTWARE'] %>
Generated: <%= Time.now.to_s %>

--- Boot variables
<% capture.each do |l| %><%= l %>
<% end %>
--- Request Environment
<% request.env.keys.sort.each do |k| %><%= k %> = <%= env[k] %>
<% end %>
--- Environment Variables
<% ENV.keys.sort.each do |k| %><%= k %> = <%= ENV[k] %>
<% end %>
--- System Properties
<% ENV_JAVA.keys.sort.each do |k| %><%= k %> = <%= ENV_JAVA[k] %>
<% end if defined?(ENV_JAVA) %>
<% if defined?($servlet_context) %>
--- Context Init Parameters
<% $servlet_context.init_parameter_names.sort.each do |k| %><%= k %> = <%= $servlet_context.get_init_parameter(k) %>
<% end ; end %>
